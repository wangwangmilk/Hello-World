/* components/j-tab-control/j-tab-control.wxss */
.tab-control {
  display: flex;
  height: 44px;
  line-height: 44px;
  background-color:#fff;
}
.tab-control view {
  flex: 1;
  text-align: center;
}
.active {
  color: #ff5777;
}
.active text {
  padding: 10rpx 10rpx ;
  border-bottom: 3px solid #ff5777;
}
